{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "8.11.4",
    "npm": "6"
  },
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "d3": "^4.13.0",
    "firebase": "^5.8.5",
    "moment": "^2.29.4",
    "particles.js": "^2.0.0",
    "vue": "^2.6.8",
    "vue-router": "^3.0.2",
    "vue-slider-component": "^2.8.14",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.1",
    "@vue/cli-plugin-eslint": "^3.4.1",
    "@vue/cli-service": "^3.4.1",
    "@vue/eslint-config-prettier": "^3.0.5",
    "lint-staged": "^6.1.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^6.0.7",
    "vue-template-compiler": "^2.6.8"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
